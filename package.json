{
  "name": "@gatsby-tv/components",
  "description": "Gatsby shared React components for web/mobile/desktop clients.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-tv/components.git"
  },
  "version": "0.3.34",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "types": "tsc --noEmit",
    "types:watch": "yarn run check -- --watch",
    "clean": "rm -rf dist",
    "build": "yarn run build:tsc && yarn run build:babel",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:babel": "babel lib --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@gatsby-tv/icons": "^0.3.3",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@storybook/theming": "^6.0.21",
    "@types/color": "^3.0.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/webfontloader": "^1.6.32",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-lorem-ipsum": "^1.4.7",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "color": "^3.1.2",
    "react-popper": "^2.2.3"
  }
}
